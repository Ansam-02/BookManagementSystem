[Unit]
Description=Simple App service
After=syslog.target
After=network.target

[service]
AmbientCapabilities=CAP_NET_BIND_SERVICE
Type=simple
User=ubuntu 
Group=ubuntu
WorkingDirectory=/home/ubuntu/app
ExecStart=/usr/bin/node ./dist/index.js
Restart=always
Environment=PORT=80


[Install]
WantedBy=multi-user.target
